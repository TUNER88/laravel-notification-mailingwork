{
    "name": "laravel-notification-channels/mailingwork",
    "description": "mailingwork Notifications Channel for Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/mailingwork",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Pauli",
            "email": "pauli@ironshark.de",
            "homepage": "https://github.com/TUNER88",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Mailingwork\\": "src"
        }
